{
  "metadata": {
    "DEBUG": "false",
    "PROJECT_WORKSPACE": "springboot-maven",
    "ARTIFACT_OUTPUT": "true",
    "ARTIFACT_OUTPUT_DESTINATION": "target",
    "COMPUTE_TYPE": "SMALL"
  },
  "label": {
    "customer": "mrwconsulting",
    "project": "springboot-maven"
  },
  "git": {
    "owner": "mrwconsulting",
    "branch": "main",
    "repository": "shinto-samples",
    "codeStarId": "e196c5a3-4efd-4f35-98fe-16b3f6f65e6b"
  },
  "buildSpec": {
    "type": "JAVA",
    "packageManager": "MAVEN",
    "codeScanType": ["CODACY","SNYK"],
    "env": {
      "PACKAGE_WRAPPER_ENABLED": "true"
    }
  },
  "imageBuilder": {
    "type": "BUILDPACK",
    "enabled": true,
    "imageScanType": ["ECR","SNYK"],
    "env": {
      "IMAGESCAN_FINDINGS_DISABLED": "true"
    }
  },
  "deploy": {
    "type": "CDK8S",
    "enabled": true,
    "global": {
      "region": ["us-east-1"],
      "approval": true,
      "env": {
        "PACKAGE_MANAGER": "NPM"
      }
    },
    "stage": [
      {
        "target": "DEV",
        "account": "xxxxxxxxxxxxxxxx"
      }
    ]
  }
}